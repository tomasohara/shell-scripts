# pause-for-enter: EXITS AFTER PRESSING ENTER, WITH A MESSAGE
$ function pause-for-enter () {
$     local message="$1"
$     if [ "$message" = "" ]; then message="Press enter to continue"; fi
$     read -p "$message "
$ }

